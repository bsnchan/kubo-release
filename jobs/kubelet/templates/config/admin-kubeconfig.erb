apiVersion: v1
kind: Config
clusters:
- cluster:
    certificate-authority: "/var/vcap/jobs/kubelet/config/ca.pem"
    server: https://master.cfcr.internal:8443
  name: kubernetes
contexts:
- context:
    cluster: kubernetes
    user: kubernetes-admin
  name: kubernetes-admin
current-context: kubernetes-admin
users:
- name: kubernetes-admin
  user:
    token: "<%= p("admin-password") %>"
